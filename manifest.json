{
    "manifest_version": 2,
    "name": "elliots extension",
    "description": "this is a description",
    "version": "0.1",
    "content_scripts": [{
        "matches": [
            "*://*/*",
            "file://*"
        ],
        "css": [
            "style.css"
        ],
        "js": [
            "content.js"
        ],
        "run_at": "document_end"
    }],
    "icons": {
        "128": "images/icon.png"
    }
}